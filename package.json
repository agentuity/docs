{
	"name": "docs",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prebuild": "node scripts/generate-docs-json.js",
		"build": "next build",
		"build:worker": "opennextjs-cloudflare build",
		"build:production": "NODE_ENV=production next build",
		"dev": "PORT=3201 next dev",
		"start": "next start",
		"postinstall": "fumadocs-mdx",
		"preview": "opennextjs-cloudflare && wrangler dev",
		"deploy": "opennextjs-cloudflare && wrangler deploy --keep-vars",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"validate-env": "node -e \"require('./lib/env').validateEnv()\"",
		"terminal-server": "node scripts/start-terminal-server.js",
		"coding-server": "echo 'Tutorial APIs are now integrated into Next.js at /api/tutorials'",
		"dev:full": "concurrently \"npm run terminal-server\" \"npm run dev\""
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-visually-hidden": "^1.2.3",
		"@types/ws": "^8.18.1",
		"@xterm/addon-fit": "^0.10.0",
		"@xterm/xterm": "^5.5.0",
		"axios": "^1.7.0",
		"cmdk": "^1.1.1",
		"cors": "^2.8.5",
		"express": "^4.18.0",
		"fumadocs-core": "15.2.6",
		"fumadocs-mdx": "11.5.8",
		"fumadocs-twoslash": "^3.1.0",
		"fumadocs-ui": "^15.6.1",
		"geist": "^1.4.2",
		"gray-matter": "^4.0.3",
		"lucide-react": "^0.525.0",
		"mermaid": "^11.6.0",
		"motion": "^12.12.2",
		"next": "15.3.0",
		"node-pty": "^1.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-markdown": "^10.1.0",
		"remark-gfm": "^4.0.1",
		"twoslash": "^0.3.1",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"@opennextjs/cloudflare": "^1.3.0",
		"@tailwindcss/postcss": "^4.1.3",
		"@types/mdx": "^2.0.13",
		"@types/node": "22.10.9",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3",
		"concurrently": "^9.2.0",
		"eslint": "^8",
		"eslint-config-next": "15.1.6",
		"postcss": "^8.5.1",
		"tailwindcss": "^4.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"wrangler": "^4.10.0"
	},
	"overrides": {
		"esbuild": ">=0.25.0"
	}
}
