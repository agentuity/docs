{
	"name": "docs",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"prebuild": "node scripts/generate-docs-json.js",
		"build": "next build",
		"build:worker": "opennextjs-cloudflare build",
		"build:production": "NODE_ENV=production next build",
		"dev": "PORT=3201 next dev",
		"start": "next start",
		"format": "npx @biomejs/biome format",
		"format:fix": "npx @biomejs/biome format --write",
		"organize-imports": "npx @biomejs/biome check --formatter-enabled=false --linter-enabled=false --write",
		"lint": "npx @biomejs/biome lint",
		"lint:fix": "npx @biomejs/biome lint --fix",
		"postinstall": "fumadocs-mdx",
		"preview": "opennextjs-cloudflare && wrangler dev",
		"deploy": "opennextjs-cloudflare && wrangler deploy --keep-vars",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"validate-env": "node -e \"require('./lib/env').validateEnv()\""
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.31",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-visually-hidden": "^1.2.3",
		"@types/uuid": "^10.0.0",
		"ai": "^5.0.45",
		"allotment": "^1.20.4",
		"fumadocs-core": "15.2.6",
		"fumadocs-mdx": "11.5.8",
		"fumadocs-twoslash": "^3.1.0",
		"fumadocs-ui": "^15.6.1",
		"geist": "^1.4.2",
		"gray-matter": "^4.0.3",
		"lucide-react": "^0.525.0",
		"mermaid": "^11.6.0",
		"motion": "^12.12.2",
		"next": "^15.5.2",
		"node-pty": "^1.0.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-markdown": "^10.1.0",
		"remark-gfm": "^4.0.1",
		"swr": "^2.3.6",
		"twoslash": "^0.3.1",
		"uuid": "^11.1.0",
		"zod": "^4.1.9"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@opennextjs/cloudflare": "^1.3.0",
		"@tailwindcss/postcss": "^4.1.3",
		"@types/mdx": "^2.0.13",
		"@types/node": "22.10.9",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3",
		"ignore-loader": "^0.1.2",
		"postcss": "^8.5.1",
		"tailwindcss": "^4.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"wrangler": "^4.10.0"
	},
	"overrides": {
		"esbuild": ">=0.25.0"
	}
}
